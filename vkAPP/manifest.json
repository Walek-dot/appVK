{
    "manifest_version": 3,
    "name": "VK music keaper",
    "version": "1.0",
    "description": "my fierst app",
    "icons": {
        "128": "128_icon.png",
        "48": "48_icon.png",
        "16": "16_icon.png"
    },
    "content_scripts": [
        {
            "matches": [ "https://*.vk.com/*" ],
            "js": ["jquery-3.6.1.min.js"],
            "run_at": "document_start"
        },
        {
            "matches": [ "https://*.vk.com/*" ],
            "js": ["app.js"],
            "run_at": "document_end"
        }
    ]
 } 